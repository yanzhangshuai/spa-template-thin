{
  "name": "spa-template-thin",
  "type": "module",
  "version": "1.0.0",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "repository": "https://github.com/yanzhangshuai/spa-template-thin.git",
  "web-types": "./src/components/web-types.json",
  "scripts": {
    "vue-tsc": "vue-tsc --noEmit --skipLibCheck",
    "vite-build": "vite build --config ./build/vite.config.ts",
    "dev": "vite --config ./build/vite.config.ts",
    "build": "npm-run-all vue-tsc && npm-run-all vite-build",
    "build:gz": "cross-env VITE_GZIP=true vite build --config ./build/vite.config.ts",
    "serve": "npm-run-all vite-build && vite preview --config ./build/vite.config.ts",
    "deploy": "npm-run-all build && docker build -t spa-template-thin -f ./deploy/Dockerfile .",
    "report": "cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --passWithNoTests --config ./test/vitest.config.ts"
  },
  "dependencies": {
    "@mwjz/pinia-storage": "^0.0.3",
    "@pinia/plugin-debounce": "^1.0.1",
    "@tanstack/vue-query": "^5.53.3",
    "@vueuse/core": "^11.0.3",
    "ant-design-vue": "4.x",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "pinia": "^2.2.2",
    "pug": "^3.0.3",
    "typescript": "^5.5.4",
    "unocss": "^0.62.3",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@optimize-lodash/rollup-plugin": "^5.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.2",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/language-plugin-pug": "^2.1.4",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue-pug": "^0.6.2",
    "globby": "^14.0.2",
    "jsdom": "^25.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.21.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.2",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.7",
    "vite": "^5.4.2",
    "vite-imagetools": "^7.0.4",
    "vite-plugin-compression2": "^1.2.0",
    "vite-plugin-restart": "^0.4.1",
    "vite-plugin-static-copy": "^1.0.6",
    "vite-plugin-vue-devtools": "^7.3.9",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
