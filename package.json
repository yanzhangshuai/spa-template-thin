{
  "name": "spa-template-thin",
  "type": "module",
  "version": "1.0.0",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "repository": "https://github.com/yanzhangshuai/spa-template-thin.git",
  "web-types": "./src/components/web-types.json",
  "scripts": {
    "vite-build": "vite build --config ./build/vite.config.ts",
    "dev": "vite --config ./build/vite.config.ts",
    "build": "vue-tsc -b && npm-run-all vite-build",
    "build:gz": "cross-env VITE_GZIP=true vite build --config ./build/vite.config.ts",
    "serve": "npm-run-all vite-build && vite preview --config ./build/vite.config.ts",
    "deploy": "npm-run-all build && docker build -t spa-template-thin -f ./deploy/Dockerfile .",
    "report": "cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --passWithNoTests --config ./test/vitest.config.ts"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.59.20",
    "@vueuse/core": "^11.2.0",
    "ant-design-vue": "4.x",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "pinia": "^2.2.6",
    "typescript": "^5.6.3",
    "unocss": "^0.64.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "@optimize-lodash/rollup-plugin": "^5.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globby": "^14.0.2",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.26.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.2",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.11",
    "vite-imagetools": "^7.0.4",
    "vite-plugin-compression2": "^1.3.1",
    "vite-plugin-restart": "^0.4.1",
    "vite-plugin-static-copy": "^2.1.0",
    "vite-plugin-vue-devtools": "^7.6.4",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  }
}
