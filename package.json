    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "globby": "^14.1.0",
    "jsdom": "^26.1.0",
    "only-allow": "^1.2.1",
    "rollup": "^4.40.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "ts-node": "^10.9.2",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.3",
    "vite-imagetools": "^7.0.5",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-inspect": "^11.0.1",
