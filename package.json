  "license": "MIT",
  "repository": "https://github.com/yanzhangshuai/spa-template-thin.git",
  "web-types": "./src/components/web-types.json",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "cross-env VITE_GZIP=true vite build",
    "inspect": "cross-env VITE_INSPECT=true vite build",
    "serve": "vite build && vite preview",
    "deploy": "docker-compose -f ./deploy/docker-compose.yml up -d",
    "report": "cross-env VITE_REPORT=true vite build",
    "lint": "eslint . --fix",
    "test": "vitest --passWithNoTests --ui ",
    "test:coverage": "vitest --passWithNoTests --coverage"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.74.6",
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^13.1.0",
    "ant-design-vue": "4.x",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "less": "^4.3.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.2",
    "typescript": "^5.8.3",
    "unocss": "^66.0.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.15.2",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-legacy": "^6.1.0",
