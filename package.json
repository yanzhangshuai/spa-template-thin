{
	"name": "spa-template-thin",
	"version": "1.0.0",
	"repository": "https://github.com/yanzhangshuai/spa-template-thin.git",
	"author": "yanzhangshuai<yanzhangshuai@126.com>",
	"license": "MIT",
	"web-types": "./src/component/web-types.json",
	"scripts": {
		"vue-tsc": "     vue-tsc --noEmit --skipLibCheck",
		"vite-build": "  vite build --config ./build/vite.config.ts",
		"dev": "         vite --config ./build/vite.config.ts",
		"build": "       npm-run-all vue-tsc && npm-run-all vite-build",
		"build:br": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=brotli npm-run-all vite-build",
		"build:gz": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=gzip   npm-run-all vite-build",
		"serve": "       npm-run-all vite-build && vite preview --config ./build/vite.preview.ts",
		"deploy": "      npm-run-all build:gz && docker build -t spa-template-thin -f ./deploy/Dockerfile .",
		"lint:js": "     eslint    ./{src,test,type,build}/**/*.{js,ts,tsx,vue} --fix --ignore-path ./standard/.eslintignore",
		"lint:style": "  stylelint ./src/**/*.{css,less,vue}           --fix",
		"lint": "        npm-run-all --parallel lint:*",
		"test": "        vitest --passWithNoTests --config ./test/vitest.config.ts",
		"report": "      cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
		"report:test": " vitest run --passWithNoTests --config ./test/vitest.config.ts --coverage  && open-cli ./report/test/index.html",
		"report:windi": "windicss-analysis"
	},
	"dependencies": {
		"@mwjz/asker": "^0.0.3",
		"@mwjz/pinia-storage": "^0.0.3",
		"@mwjz/utils": "^0.1.8",
		"@pinia/plugin-debounce": "^0.1.0",
		"@vueuse/core": "^9.3.0",
		"axios": "^0.27.2",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.5",
		"less": "^4.1.3",
		"pinia": "^2.0.22",
		"query-string": "^7.1.1",
		"ramda": "^0.28.0",
		"rxjs": "^7.5.7",
		"ts-debounce": "^4.0.0",
		"typescript": "^4.8.3",
		"vue": "^3.2.39",
		"vue-router": "^4.1.5",
		"windicss": "^3.5.6"
	},
	"devDependencies": {
		"@antfu/eslint-config-vue": "^0.27.0",
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-node-resolve": "^14.1.0",
		"@types/crypto-js": "^4.1.1",
		"@types/glob": "^8.0.0",
		"@types/node": "^18.7.23",
		"@types/ramda": "^0.28.15",
		"@typescript-eslint/parser": "^5.38.1",
		"@vitejs/plugin-legacy": "^2.2.0",
		"@vitejs/plugin-vue": "^3.1.0",
		"@vitejs/plugin-vue-jsx": "^2.0.1",
		"@vitest/coverage-c8": "^0.23.4",
		"@vue/runtime-core": "^3.2.39",
		"@vue/test-utils": "^2.0.2",
		"autoprefixer": "^10.4.12",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.13",
		"eslint": "^8.24.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-vue": "^9.5.1",
		"glob": "^8.0.3",
		"jsdom": "^20.0.0",
		"lint-staged": "^13.0.3",
		"npm-run-all": "^4.1.5",
		"open-cli": "^7.0.1",
		"postcss": "^8.4.16",
		"postcss-html": "^1.5.0",
		"postcss-less": "^6.0.0",
		"rollup": "^2.79.1",
		"rollup-plugin-visualizer": "^5.8.2",
		"stylelint": "^14.12.1",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^28.0.0",
		"stylelint-config-standard-vue": "^1.0.0",
		"terser": "^5.15.0",
		"ts-node": "^10.9.1",
		"vite": "^3.1.3",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-windicss": "^1.8.8",
		"vitest": "^0.23.4",
		"vue-tsc": "^0.40.13",
		"windicss-analysis": "^0.3.5"
	},
	"eslintConfig": {
		"extends": "./standard/.eslintrc.js"
	},
	"stylelint": {
		"extends": "./standard/.stylelintrc.js"
	}
}
