{
	"name": "spa-template-thin",
	"version": "1.0.0",
	"repository": "https://github.com/yanzhangshuai/spa-template-thin.git",
	"author": "yanzhangshuai<yanzhangshuai@126.com>",
	"license": "MIT",
	"scripts": {
		"vue-tsc": "     vue-tsc --noEmit --skipLibCheck",
		"vite-build": "  vite build --config ./build/vite.config.ts",
		"dev": "         vite --config ./build/vite.config.ts",
		"build": "       npm-run-all vue-tsc && npm-run-all vite-build",
		"build:br": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=brotli npm-run-all vite-build",
		"build:gz": "    npm-run-all vue-tsc && cross-env VITE_BUILD_COMPRESS=gzip   npm-run-all vite-build",
		"serve": "       npm-run-all vite-build && vite preview --config ./build/vite.preview.ts",
		"deploy": "      npm-run-all build:gz && docker build -t spa-template-thin -f ./deploy/Dockerfile .",
		"lint:js": "     eslint    ./{src,test,type,build}/**/*.{js,ts,tsx,vue} --fix",
		"lint:style": "  stylelint ./src/**/*.{css,less,vue}           --fix",
		"lint": "        npm-run-all --parallel lint:*",
		"test": "        vitest --passWithNoTests --config ./test/vitest.config.ts",
		"report": "      cross-env VITE_REPORT=true vite build --config ./build/vite.config.ts",
		"report:test": " vitest run --passWithNoTests --config ./test/vitest.config.ts --coverage  && open-cli ./report/test/index.html",
		"report:windi": "windicss-analysis"
	},
	"dependencies": {
		"@mwjz/asker": "^0.0.3",
		"@mwjz/pinia-storage": "^0.0.3",
		"@pinia/plugin-debounce": "^0.0.2",
		"@vueuse/core": "^8.3.1",
		"axios": "^0.27.2",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.1",
		"less": "^4.1.2",
		"pinia": "^2.0.13",
		"query-string": "^7.1.1",
		"ramda": "^0.28.0",
		"rxjs": "^7.5.5",
		"ts-debounce": "^4.0.0",
		"typescript": "^4.6.4",
		"vue": "^3.2.33",
		"vue-router": "^4.0.14",
		"windicss": "^3.5.1"
	},
	"devDependencies": {
		"@antfu/eslint-config-vue": "^0.22.0",
		"@rollup/plugin-commonjs": "^22.0.0",
		"@rollup/plugin-node-resolve": "^13.2.1",
		"@types/crypto-js": "^4.1.1",
		"@types/node": "^17.0.30",
		"@types/ramda": "^0.28.11",
		"@typescript-eslint/parser": "^5.21.0",
		"@vitejs/plugin-legacy": "^1.8.1",
		"@vitejs/plugin-vue": "^2.3.1",
		"@vitejs/plugin-vue-jsx": "^1.3.10",
		"@vue/runtime-core": "^3.2.33",
		"@vue/test-utils": "^2.0.0-rc.21",
		"autoprefixer": "^10.4.5",
		"c8": "^7.11.2",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.7",
		"eslint": "^8.14.0",
		"eslint-plugin-vue": "^8.7.1",
		"jsdom": "^19.0.0",
		"lint-staged": "^12.4.1",
		"npm-run-all": "^4.1.5",
		"open-cli": "^7.0.1",
		"postcss": "^8.4.13",
		"postcss-html": "^1.4.1",
		"postcss-less": "^6.0.0",
		"rollup": "^2.71.1",
		"rollup-plugin-visualizer": "^5.6.0",
		"stylelint": "^14.8.1",
		"stylelint-config-html": "^1.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^25.0.0",
		"stylelint-config-standard-vue": "^1.0.0",
		"ts-node": "^10.7.0",
		"vite": "^2.9.6",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-windicss": "^1.8.4",
		"vitest": "^0.10.0",
		"vue-tsc": "^0.34.11",
		"windicss-analysis": "^0.3.5"
	},
	"eslintConfig": {
		"extends": "./standard/.eslintrc.js"
	},
	"stylelint": {
		"extends": "./standard/.stylelintrc.js"
	}
}
